<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.sau.bankproject.Controller.AccountController"
            prefHeight="200.0" prefWidth="350.0">
    <children>
        <Label layoutX="26.0" layoutY="20.0" text="ID:" />
        <Label layoutX="26.0" layoutY="50.0" text="BRANCH:" />
        <Label layoutX="26.0" layoutY="79.0" text="BALANCE:" />

        <TextField fx:id="accountid" layoutX="89.0" layoutY="16.0" />
        <TextField fx:id="branch" layoutX="89.0" layoutY="46.0" />
        <TextField fx:id="balance" layoutX="89.0" layoutY="76.0" />

        <Button fx:id="getAccount" layoutX="267.0" layoutY="16.0" mnemonicParsing="false" onAction="#getAccount" text="GET" />

        <Button fx:id="saveAccount" layoutX="34.0" layoutY="139.0" mnemonicParsing="false" onAction="#saveAccount" text="SAVE" />
        <Button fx:id="updateAccount" layoutX="96.0" layoutY="138.0" mnemonicParsing="false" onAction="#updateAccount" text="UPDATE" />
        <Button fx:id="deleteAccount" layoutX="178.0" layoutY="138.0" mnemonicParsing="false" onAction="#deleteAccount" text="DELETE" />

        <Button fx:id="close" layoutX="259.0" layoutY="137.0" mnemonicParsing="false" onAction="#close" text="CLOSE" />
        <Button fx:id="clearAccount" layoutX="261.0" layoutY="77.0" mnemonicParsing="false" onAction="#clearAccount" text="CLEAR" />
    </children>
</AnchorPane>