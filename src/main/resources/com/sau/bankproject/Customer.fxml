<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.sau.bankproject.Controller.CustomerController"
            prefHeight="220.0" prefWidth="350.0"> <children>
    <Label layoutX="26.0" layoutY="20.0" text="ID:" />
    <Label layoutX="26.0" layoutY="50.0" text="NAME:" />
    <Label layoutX="26.0" layoutY="79.0" text="ADDRESS:" />
    <Label layoutX="26.0" layoutY="109.0" text="CITY:" /> <TextField fx:id="customerId" layoutX="89.0" layoutY="16.0" />
    <TextField fx:id="name" layoutX="89.0" layoutY="46.0" />
