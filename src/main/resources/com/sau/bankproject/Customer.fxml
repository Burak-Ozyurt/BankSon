    <Button fx:id="deleteCustomer" layoutX="178.0" layoutY="140.0" mnemonicParsing="false" onAction="#deleteCustomer" text="DELETE" />
    <Button fx:id="close" layoutX="259.0" layoutY="140.0" mnemonicParsing="false" onAction="#close" text="CLOSE" />

